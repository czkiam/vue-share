    "express": "^4.17.1",
    "graphql": "^14.2.1",
    "jsonwebtoken": "^9.0.0",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "mongoose": "^5.2.6"
  },
  "devDependencies": {
    "concurrently": "^3.6.0",
    "nodemon": "^1.18.1"
